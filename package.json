  "main": "index.js",
  "scripts": {
    "build:dev": " rm -rf ./.dev && mkdir -p ./.dev/client &&  export NODE_ENV=dev WATCH=true;  node scripts/build.js",
    "build:prod": " rm -rf ./.build && mkdir -p ./.build/client && export NODE_ENV=production; node scripts/build.js",
    "start:dev": "export IGNORE_MODERN_BUILD=true NODE_ENV=dev WATCH=true HOT_RELOAD=true; node scripts/start",
    "start:dev-modern": "export NODE_ENV=dev WATCH=true HOT_RELOAD=true; node scripts/start",
    "start:dev:inspect": "export IGNORE_MODERN_BUILD=true NODE_ENV=dev WATCH=true HOT_RELOAD=true INSPECT=true; node scripts/start",
    "start:dev-modern:inspect": "export NODE_ENV=dev WATCH=true HOT_RELOAD=true INSPECT=true; node scripts/start",
    "start:prod": "export NODE_ENV=production; node .build/server/server.js",
    "lint-staged": "lint-staged",
    "format": "./node_modules/.bin/prettier --write",
    "lint-all": "npm run format-all && npm run lint",
    "format-all": "./node_modules/.bin/prettier --write --single-quote --print-width=120 \"**/*.{js,jsx}\"",
    "lint": "./node_modules/eslint/bin/eslint.js . --ext .js --ext .jsx --fix || true",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
